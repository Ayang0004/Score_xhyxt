<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xhyxt.mapper.TeachConditionMapper">


		<delete id="teachTextooksDelete">
			delete from tb_teachcondition where id in
			<foreach collection="array" item="id" open="(" close=")" separator=",">
				#{id}
			</foreach>
		</delete>
		
		<delete id="teachNetDelete">
			delete from tb_teachcondition where id in
			<foreach collection="array" item="id" open="(" close=")" separator=",">
				#{id}
			</foreach>
		</delete>
		
		
</mapper>